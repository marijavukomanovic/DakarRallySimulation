<Window x:Class="WPF__Client.AddVehicle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF__Client"
        mc:Ignorable="d"
        Title="AddVehicle" Height="300" Width="200">
    <Grid>
        <StackPanel HorizontalAlignment="Center" Height="300" Width="200">
           
            <Label Content="Team Name:" Height="30" Width="100"/>
            <TextBox x:Name="TeamName" Height="30" Width="100" Margin="10,10,0,0"/>
            <Label Content="Model:" Height="30" Width="100"/>
                <TextBox x:Name="VehicleModel" Height="30" Width="100" Margin="10,10,0,0"/>
            <Label Content="Date (year)" Height="30" Width="100"/>
                <TextBox x:Name="Date" Height="30" Width="100" Margin="10,10,0,0"/>
            <Button x:Name="AddVehicle" Content="Add Vehicle" Height="30" Width="97" Margin="10,20,0,0"/>
        </StackPanel>

    </Grid>
</Window>
